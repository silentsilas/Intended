{
  "compilerOptions": {
    "allowJs": true,
    "sourceMap": true,
    "target": "es5",
    "strict": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "baseUrl": ".",
    "types": [
      "quasar",
      "node"
    ],
    "lib": [
      "ES2015",
      "DOM"
    ],
    "paths": {
      "~/*": ["./*"],
		  "@/*": ["src/*"]
    }
  },
  "exclude": ["node_modules"]
}
